{
  "name": "api-fort",
  "version": "2.0.0",
  "description": "The treefort public API hosted on AWS Lambda.",
  "main": "index.js",
  "dependencies": {
    "aws-sdk": "^2.121.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.4.6",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "serverless-webpack": "^3.0.0",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "scripts": {
    "build": "serverless webpack --out dist",
    "deploy:all": "serverless deploy",
    "deploy:events": "serverless deploy --function events",
    "deploy:lines": "serverless deploy --function lines",
    "deploy:performers": "serverless deploy --function performers",
    "deploy:playlists": "serverless deploy --function playlists",
    "deploy:schedules": "serverless deploy --function schedules",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "logs:events": "serverless logs -f events",
    "logs:lines": "serverless logs -f lines",
    "logs:performers": "serverless logs -f performers",
    "logs:playlists": "serverless logs -f playlists",
    "logs:schedules": "serverless logs -f schedules",
    "precommit": "lint-staged",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treefortmusicfest/api-fort.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "api"
  ],
  "author": "bdietz, junger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/treefortmusicfest/api-fort/issues"
  },
  "homepage": "https://github.com/treefortmusicfest/api-fort#readme"
}
